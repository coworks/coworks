<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="approval">

	<select id="selectApprovalFormFolderList" resultType="string">
		select distinct aform_folderName from approvalForm
	</select>

	<select id="selectApprovalFormList" parameterType="string" resultType="approvalForm">
		select * from approvalForm where aform_folderName=#{aform_folderName}
	</select>
	
	


</mapper>
